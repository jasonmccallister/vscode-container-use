{
  "name": "container-use",
  "displayName": "container-use",
  "description": "Development environments for coding agents.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "extension",
    "container",
    "development",
    "environments",
    "coding agents",
    "container-use",
    "dagger"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "container-use.install",
        "title": "Install",
        "category": "Container Use",
        "shortTitle": "Install"
      },
      {
        "command": "container-use.list",
        "title": "List Environments",
        "category": "Container Use",
        "shortTitle": "List"
      },
      {
        "command": "container-use.log",
        "title": "Show the log for an environment",
        "category": "Container Use",
        "shortTitle": "Log"
      },
      {
        "command": "container-use.delete",
        "title": "Delete an environment",
        "category": "Container Use",
        "shortTitle": "Delete"
      },
      {
        "command": "container-use.instructions",
        "title": "Add Copilot instructions",
        "category": "Container Use",
        "shortTitle": "Instructions"
      },
      {
        "command": "container-use.terminal",
        "title": "Container Use: Terminal",
        "category": "Container Use",
        "shortTitle": "Terminal"
      },
      {
        "command": "container-use.watch",
        "title": "Watch git log output",
        "category": "Container Use",
        "shortTitle": "Watch"
      },
      {
        "command": "container-use.merge",
        "title": "Merge environments into current git branch",
        "category": "Container Use",
        "shortTitle": "Merge"
      },
      {
        "command": "container-use.resetPreferences",
        "title": "Reset the extension preferences",
        "category": "Container Use",
        "shortTitle": "Reset Preferences"
      }
    ],
    "mcpServerDefinitionProviders": [
      {
        "id": "container-use",
        "label": "Development environments for coding agents."
      }
    ],
    "configuration": {
      "title": "Container Use",
      "properties": {
        "container-use.installMethod": {
          "type": "string",
          "enum": [
            "brew",
            "curl"
          ],
          "enumDescriptions": [
            "Install using Homebrew (recommended for macOS/Linux)",
            "Install using curl script"
          ],
          "default": "curl",
          "description": "Preferred installation method for Container Use binary"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "sinon": "^21.0.0",
    "typescript": "^5.8.3"
  }
}